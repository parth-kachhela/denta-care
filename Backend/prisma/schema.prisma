generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Appointment {
  id           Int      @id @default(autoincrement())
  fullName     String
  mobileNumber String
  helpWith     String?
  message      String?
  createdAt    DateTime @default(now())
}
